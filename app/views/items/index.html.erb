<div class="row">
  <h2>アイテム一覧</h2>
  <%= link_to 'アイテム登録', new_item_path, class: 'btn btn-primary ml-3' %>
</div>
<table class="table">
  <thead>
    <tr>
      <th>アイテム名</th>
      <th>登録日時</th>
      <td></td>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
          <td><%= item.name %></td>
          <td><%= item.created_at %></td>
          <td><%= link_to '編集', edit_item_path(item), class: 'btn btn-primary' %>
      </tr>
    <% end %>
  </tbody>
</table>
